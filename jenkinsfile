pipeline
{
agent any
stages
{
    stage('hello jenkins')
    {
        steps{sh 'echo hi jenkins'}
    }
    
    stage('execute test')
    {
        parallel
        {
            stage(execute unit test)
            {steps{sh 'echo execute unit test'}}
            stage(execute component test)
            {
                steps{sh 'echo execute component test'}
            }
        }
    }
}
}